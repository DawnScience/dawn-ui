<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="org.dawb.workbench.ui.beta.views.category"
            class="org.dawnsci.processing.ui.ProcessingView"
            icon="icons/image-sharpen.png"
            id="org.dawnsci.processing.ui.processingView"
            name="Processing"
            restorable="true">
      </view>
      <view
            category="org.dawb.workbench.ui.beta.views.category"
            class="org.eclipse.ui.views.properties.PropertySheet"
            icon="icons/image--pencil.png"
            id="org.dawnsci.processing.ui.propertySheet"
            name="Model"
            restorable="true">
      </view>
      <view
            class="org.dawnsci.processing.ui.DataFileSliceView"
            id="org.dawnsci.processing.ui.DataFileSliceView"
            name="Data Slice View"
            restorable="true">
      </view>
      <view
            class="uk.ac.diamond.scisoft.analysis.rcp.views.PlotView"
            id="org.dawnsci.processing.ui.input"
            name="Input"
            restorable="true">
      </view>
      <view
            class="uk.ac.diamond.scisoft.analysis.rcp.views.PlotView"
            id="org.dawnsci.processing.ui.output"
            name="Output"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.dawnsci.processing.ui.ProcessingPerspective"
            id="org.dawnsci.processing.ui.ProcessingPerspective"
            name="Processing">
      </perspective>
   </extension>

</plugin>
