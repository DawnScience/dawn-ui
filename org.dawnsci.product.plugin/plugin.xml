<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="DAWN"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="DAWN Science">
         <property
               name="appName"
               value="DAWN Science">
         </property>
         <property
               name="aboutImage"
               value="icons/Dawn-210x238-about.png">
         </property>
         <property
               name="aboutText"
               value="Data Analysis WorkbeNch&#x0A;Copyright, 2010 - 2021, Diamond Light Source&#x0A;&#x0A;Version 2.22.0">
         </property>
         <property
               name="windowImages"
               value="icons/dawn-icon-16.png,icons/dawn-icon-32.png,icons/dawn-icon-48.png">
         </property>
         <property
               name="startupForegroundColor"
               value="00AA00">
         </property>
         <property
               name="startupMessageRect"
               value="5,5,300,20">
         </property>
         <property
               name="startupProgressRect"
               value="5,235,590,20">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.installationPages">
      <page
            class="org.dawnsci.product.plugin.about.ReleaseNotesPage"
            id="org.dawnsci.product.plugin.about.releasenotespage"
            name="Release Notes">
      </page>
   </extension>
</plugin>
